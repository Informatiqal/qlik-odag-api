{
  "name": "qlik-odag-api",
  "version": "0.1.0",
  "description": "Interact with Qlik Sense ODAG API (QSEoW)",
  "author": {
    "email": "info@informatiqal.com",
    "name": "Informatiqal @informatiqal",
    "url": "https://informatiqal.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "start": "node dist/app.js",
    "prestart": "npm run build-tsc",
    "build-tsc": "tsc",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "docs": "npx typedoc src/index.doc.ts --name \"Qlik ODAG API\" --excludePrivate --hideGenerator --darkHighlightTheme dark-plus",
    "test": "vitest run"
  },
  "engines": {
    "node": ">=14.19.1"
  },
  "engineStrict": true,
  "keywords": [
    "qlik",
    "sense",
    "qlik-sense",
    "qlik sense",
    "odag api",
    "odag",
    "api"
  ],
  "files": [
    "dist",
    "*.json",
    "*.md",
    "LICENSE",
    "CHANGELOG.md",
    "!tsconfig.json",
    "!tsconfig.test.json",
    "!Notes.md",
    "!renovate.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/informatiqal/qlik-odag-api.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/informatiqal/qlik-odag-api/issues"
  },
  "homepage": "https://github.com/informatiqal/qlik-odag-api#readme",
  "dependencies": {
    "qlik-rest-api": "^1.7.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/node": "^18.16.1",
    "dotenv": "^16.0.3",
    "esm": "^3.2.25",
    "nyc": "^15.1.0",
    "rollup": "^3.21.0",
    "rollup-plugin-delete": "^2.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4",
    "vitest": "^0.34.0"
  }
}
